		<!--ObjectType-->
		<COLUMN>
			<ID>12</ID>
			<DESCRIPTION>Objekttyp</DESCRIPTION>
			<BESTPRACTICE/>
		</COLUMN>
		<!--ObjectName-->
		<COLUMN>
			<ID>13</ID>
			<DESCRIPTION>Objektname</DESCRIPTION>
			<BESTPRACTICE/>
