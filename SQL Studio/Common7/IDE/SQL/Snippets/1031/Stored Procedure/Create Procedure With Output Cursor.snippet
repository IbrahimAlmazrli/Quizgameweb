    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
</_locDefinition>
	<CodeSnippet Format="1.0.0">
	<Header>
	<Title>Prozedur mit CURSOR OUTPUT-Parameter erstellen</Title>
        <Shortcut></Shortcut>
	<Description>Erstellt eine gespeicherte Prozedur mit einem CURSOR OUTPUT-Parameter.</Description>
	<Author>Microsoft Corporation</Author>
	<SnippetTypes>
		<SnippetType>Expansion</SnippetType>
	</SnippetTypes>
	</Header>
	<Snippet>
		<Declarations>
			<Literal>
				<ID>SchemaName</ID>
				<ToolTip>Name des Schemas</ToolTip>
				<Default>dbo</Default>
			</Literal>
			<Literal>
				<ID>storedprocname</ID>
				<ToolTip>Name der gespeicherten Prozedur</ToolTip>
				<Default>Sample_Procedure</Default>
			</Literal>
			<Literal>
				<ID>Param1</ID>
				<ToolTip>Name des OUTPUT-Parameters</ToolTip>
				<Default>sample_procedure_cursor</Default>
			</Literal>
			<Literal>
				<ID>datatype1</ID>
				<ToolTip>Datentyp des OUTPUT-Parameters</ToolTip>
				<Default>CURSOR</Default>
			</Literal>
		</Declarations>
		<Code Language="SQL">
			<![CDATA[CREATE PROCEDURE $SchemaName$.$storedprocname$ 
    @$Param1$ $datatype1$ VARYING OUTPUT
AS
    SET @$Param1$ = CURSOR FOR
        select 1
    OPEN @$Param1$ 
RETURN 0 $end$]]>
		</Code>
	</Snippet>
	</CodeSnippet>
</CodeSnippets>
